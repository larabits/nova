{
    "name": "larabits/nova-app",
    "description": "A Laravel Nova asset.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Larabits\\NovaApp\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larabits\\NovaApp\\AssetServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
